{
  "name": "analyze-image",
  "version": "1.0.0",
  "description": "Analyzes an image and detects potential improvements for a webpage",
  "main": "./lib/index.js",
  "scripts": {
    "test": "jest test/ --coverage --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/macbre/analyze-image.git"
  },
  "keywords": [
    "image",
    "webperf",
    "performance",
    "analyze",
    "phantomas",
    "test"
  ],
  "authors": [
    "Gaël Métais",
    "Maciej Brencz"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/macbre/analyze-image/issues"
  },
  "homepage": "https://github.com/macbre/analyze-image#readme",
  "dependencies": {
    "css-calc-transform": "1.1.0",
    "debug": "4.4.0",
    "file-type": "16.5.3",
    "htmlparser2": "10.0.0",
    "imagemin": "7.0.1",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-svgo": "9.0.0",
    "is-svg": "3.0.0",
    "media-query-fns": "2.0.0",
    "sharp": "0.32.4",
    "srcset": "4.0.0"
  },
  "devDependencies": {
    "jest": "29.7.0"
  },
  "jest": {
    "verbose": true,
    "coveragePathIgnorePatterns": [
      "test/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 80,
        "functions": 100,
        "lines": 90
      }
    }
  }
}
